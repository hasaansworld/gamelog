{
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "consistent",
  "tabWidth": 2,
  "printWidth": 100,
  "eslintIntegration": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "disableLanguages": [
      "json",
      "jsonc"
  ],
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false
}
